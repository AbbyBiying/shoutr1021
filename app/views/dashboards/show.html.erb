<h1>dashboard</h1>
<h1>welcome <%= current_user.username %></h1> 

<%= form_for @text_shout do |form| %>

  <div>
  <%= form.label :body, "shout here" %>
  </div>

  <div>
  <%= form.text_field :body %>
  </div>

  <div>
  <%= form.submit %>
  </div>

  <h2> SHOUTS! </h2>
  <%= render current_user.shouts %>

<% end %>



